set terminal pdf
set key inside left top vertical Right noreverse enhanced autotitle box
set output  "{{ plotFilename }}"
set logscale
set xlabel "Arithmetic Intensity (Flops/Byte)"
set ylabel "{{ precision }} Precision Performance (GFlops/s)" 
set xrange [0.0625:32]
set yrange [1:{{ rpeak }}]
set xtics 2 font "Verdana,10" 
set ytics 2  font "Verdana,10"


plot '{{ data }}' using 1:2 with lines lc 1  lw 4  title ""{% for code in basename %}, '' using {{(loop.index * 2) + 1}}:{{(loop.index * 2) + 2}} with lines lc {{loop.index + 1}}  lw 4  title "{{code}}"{% endfor %}
    
